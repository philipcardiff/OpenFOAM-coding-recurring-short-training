FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 0
);

regions
(
    boxToCell
    {
        min    (0.04 0.04 -1);
        max    (0.06 0.06 1);

        fieldValues
        (
            volScalarFieldValue T 100
        );
    }
);

